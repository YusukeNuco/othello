def main():
    y = int(input())
    x = int(input())
    row_lis = ['- ', '- ', '- ', '- ', '- ', '- ', '- ', '- ' ]
    board_rc = []
    for i in row_lis:
        board_rc.append(row_lis)
    board_rc = '\n'.join(map(str, board_rc))
    board_rc.replace[y][x] = 'maru '
    print(board_rc)

if __name__ == '__main__':
    main()